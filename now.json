{
  "version": 2,
  "name": "vue-paintable",
  "alias": ["vue-paintable.now.sh"],
  "builds": [
    {
      "src": "./package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "demo"
      }
    }
  ],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/demo/js/$1" },
    { "src": "^/css/(.*)", "dest": "/demo/css/$1" },
    { "src": "^/img/(.*)", "dest": "/demo/img/$1" },
    { "src": ".*", "dest": "/demo/index.html" }
  ]
}
